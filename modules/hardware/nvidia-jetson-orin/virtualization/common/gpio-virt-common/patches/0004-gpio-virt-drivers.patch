+#define CLASS_NAME "char"	  
+
+MODULE_LICENSE("GPL");						 
+MODULE_AUTHOR("Kim Sandstrom");					 
+MODULE_DESCRIPTION("NVidia GPIO Guest Proxy Kernel Module"); 
+MODULE_VERSION("0.0");				 
+
+#define GPIO_VERBOSE
